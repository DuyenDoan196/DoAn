@model IEnumerable<DoAn.Models.Product>

@{
    ViewBag.Title = "listF";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}
<h3 style="text-align:center; color: red"> GIÀY FUSAL</h3>
<div class="row">
    @foreach (var giay in Model)
    {
        <div class="col-sm-4 col-lg-4 col-md-4">
            <div class="thumbnail">
                <a href="@Url.Action("Detail", "Shoes",new { id=giay.ID_Product})"></a>
                <img src="~/img/@giay.Img" width="200" />
                <div>
                    <br />
                    <h4 style="text-align:center">
                        @giay.NameP
                    </h4>
                    <p></p>
                </div>
                <div class="rating">
                    <p>
                        <span class="glyphicon glyphicon-star"></span>
                        <span class="glyphicon glyphicon-star"></span>
                        <span class="glyphicon glyphicon-star"></span>
                    </p>
                </div>
            </div>
        </div>
    }
</div>
